{
  "displayName": "e2e",
  "rootDir": "../",
  "testMatch": ["<rootDir>/test/e2e/*.e2e.spec.ts"],
  "moduleFileExtensions": ["js", "json", "ts"],
  "transform": {
    "^.+\\.ts$": [
      "ts-jest",
      {
        "tsconfig": "tsconfig.json"
      }
    ]
  },
  "testEnvironment": "node",
  "detectOpenHandles": true,
  "logHeapUsage": true
}
